{
  "name": "analyze-css",
  "version": "0.5.0",
  "author": "Maciej Brencz <maciej.brencz@gmail.com> (https://github.com/macbre)",
  "description": "CSS selectors complexity and performance analyzer",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/macbre/analyze-css.git"
  },
  "keywords": [
    "css",
    "analyzer",
    "complexity",
    "webperf"
  ],
  "license": "BSD",
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "cli": "~0.6.2",
    "css": "^2.1.0",
    "debug": "~1.0.4",
    "fast-stats": "0.0.x",
    "glob": "~4.0.5",
    "node-sass": "~0.9.3",
    "optimist": "0.6.x",
    "slick": "~1.12.1",
    "specificity": "~0.1.3"
  },
  "devDependencies": {
    "autoprefixer": "~2.2.0",
    "glob": "~4.0.5",
    "istanbul": "~0.3.0",
    "jshint": "~2.5.2",
    "mocha": "~1.21.3"
  },
  "bin": "./bin/analyze-css.js",
  "preferGlobal": true,
  "scripts": {
    "test": "mocha -R spec",
    "lint": "jshint --verbose bin/ lib/ rules/ test/",
    "coverage": "istanbul cover  _mocha -- -R spec"
  },
  "jshintConfig": {
    "node": true,
    "strict": true,
    "validthis": true
  }
}
