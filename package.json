{
  "name": "analyze-css",
  "version": "0.10.0",
  "author": "Maciej Brencz <maciej.brencz@gmail.com> (https://github.com/macbre)",
  "description": "CSS selectors complexity and performance analyzer",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/macbre/analyze-css.git"
  },
  "keywords": [
    "analysis",
    "complexity",
    "css",
    "stylesheet",
    "webperf"
  ],
  "license": "BSD",
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "cli": "0.9.x",
    "css": "2.2.x",
    "css-shorthand-properties": "1.0.x",
    "debug": "2.2.x",
    "fast-stats": "0.0.x",
    "glob": "5.0.x",
    "node-sass": "3.3.x",
    "onecolor": "2.4.x",
    "optimist": "0.6.x",
    "slick": "~1.12.1",
    "specificity": "~0.1.3"
  },
  "devDependencies": {
    "autoprefixer-core": "6.0.x",
    "browserslist": "1.0.x",
    "istanbul": "0.3.x",
    "js-beautify": "~1.5.4",
    "jshint": "2.8.x",
    "mocha": "2.3.x"
  },
  "bin": "./bin/analyze-css.js",
  "preferGlobal": true,
  "scripts": {
    "test": "mocha -R spec",
    "lint": "jshint --verbose bin/ lib/ rules/ test/",
    "beautify": "js-beautify -r rules/*.js test/*.js test/rules/*.js bin/*.js lib/*.js lib/preprocessors/*.js",
    "coverage": "istanbul cover  _mocha -- -R spec",
    "prefixes": "DEBUG=* node data/prefixes.js"
  },
  "jshintConfig": {
    "node": true,
    "strict": true,
    "validthis": true
  }
}
